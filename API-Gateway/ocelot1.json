    {
      "DownstreamPathTemplate": "/api/employee",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7161
        }
      ],

      "UpstreamPathTemplate": "/gateway/employee",
      "UpstreamHttpMethod": [ "Post", "Get" ]
    },
    {
      "DownstreamPathTemplate": "/api/employee/{id}",
      "DownstreamScheme": "https",
